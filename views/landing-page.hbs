<main class="landing">
  <div class="bg bg-landing bg-img-1"></div>
  <div class="bg bg-landing bg-img-2"></div>
  <div class="bg bg-landing bg-img-3"></div>
  <div class="bg bg-landing bg-img-4"></div>
  <div class="flex-center-content virtual-callout-wrapper">
    <span class="virtual-callout">Due to COVID-19, the 6th Conference on Security Standards Research will be held virtually.</span>
  </div>
</main>
<div class="content flex-center-content">
  <div class="text-center">
    <h2 class="landing-section-headline section-headline observed">{{ getString "conference-name-full" }}</h2>
    <h3 class="landing-date observed">{{ getString "date" }}</h3>
  </div>
  <section class="copy-wrapper">
    {{#each (getLandingStrings)}}
      <p class="copy-block {{ blockClasses }} observed txt-light">{{{ this.copyBlock }}}</p>
    {{/each}}
  </section>
</div>
